pipeline {
agent any
stages {
	stage('SCM') {
			steps {
				echo "SCM from GIT repository"
				}
				}
	stage('Testing') { 
			steps {
				echo "Testing the code"
				}
				}
	stage('Deploying') {
			steps {
				echo "Deploying the code in production"
					}
					}
	stage('Validation') {
			steps {
				echo "Validation the code deployed in Production"
					}
					}
		}
		}
